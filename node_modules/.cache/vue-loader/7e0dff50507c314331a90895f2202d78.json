C,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;EACV,EAAE;IACA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;EACjB;AACF","file":"/home/codio/workspace/src/components/AgeCalculator.vue","sourceRoot":"","sourcesContent":["<script>\nexport default {\n  name: \"AgeCalculator\",\n\n  data() {\n    return {\n      birthday: \"\",\n      age: \"\",\n      animal: \"\",\n      zodiac: \"\",\n      sign: \"\",\n      horoscope: \"\",\n\n      us: {\n        select: false,\n      },\n    };\n  },\n\n  methods: {\n    calculateAge() {\n      if (this.us.select) {\n        let currentDate = new Date();\n        let birthDate = new Date(this.birthday);\n        let difference = currentDate - birthDate;\n        let age = Math.floor(difference / 31557600000);\n        this.age = \"You are \" + age + \" years old.\";\n      }\n    },\n\n    getZodiacSign() {\n      if (this.us.select) {\n        let birthDate = new Date(this.birthday);\n        var year = birthDate.getFullYear();\n        var calc = year % 12;\n        if (calc === 0) {\n          this.animal = \"Monkey\";\n        } else if (calc === 1) {\n          this.animal = \"Rooster\";\n        } else if (calc === 2) {\n          this.animal = \"Dog\";\n        } else if (calc === 3) {\n          this.animal = \"Pig\";\n        } else if (calc === 4) {\n          this.animal = \"Rat\";\n        } else if (calc === 5) {\n          this.animal = \"Ox\";\n        } else if (calc === 6) {\n          this.animal = \"Tiger\";\n        } else if (calc === 7) {\n          this.animal = \"Rabbit\";\n        } else if (calc === 8) {\n          this.animal = \"Dragon\";\n        } else if (calc === 9) {\n          this.animal = \"Snake\";\n        } else if (calc === 10) {\n          this.animal = \"Horse\";\n        } else if (calc === 11) {\n          this.animal = \"Sheep\";\n        }\n        this.zodiac = \"Your Chinese zodiac sign is \" + this.animal + \".\";\n      }\n    },\n\n    getHoroscope() {\n      if (this.us.select) {\n        let birthDate = new Date(this.birthday);\n        var month = birthDate.getMonth();\n        var date = birthDate.getDate();\n\n        if (month === 0) {\n          if (date >= 19 && date <= 31) {\n            this.sign = \"Aquarius\";\n          }\n        }\n        if (month === 1) {\n          if (date >= 1 && date <= 17) {\n            this.sign = \"Aquarius\";\n          }\n        }\n\n        if (month === 1) {\n          if (date >= 18 && date <= 29) {\n            this.sign = \"Pisces\";\n          }\n        }\n        if (month === 2) {\n          if (date >= 1 && date <= 19) {\n            this.sign = \"Pisces\";\n          }\n        }\n\n        if (month === 2) {\n          if (date >= 20 && date <= 31) {\n            this.sign = \"Aries\";\n          }\n        }\n        if (month === 3) {\n          if (date >= 1 && date <= 18) {\n            this.sign = \"Aries\";\n          }\n        }\n\n        if (month === 3) {\n          if (date >= 19 && date <= 30) {\n            this.sign = \"Taurus\";\n          }\n        }\n        if (month === 4) {\n          if (date >= 1 && date <= 19) {\n            this.sign = \"Taurus\";\n          }\n        }\n\n        if (month === 4) {\n          if (date >= 20 && date <= 31) {\n            this.sign = \"Gemini\";\n          }\n        }\n        if (month === 5) {\n          if (date >= 1 && date <= 19) {\n            this.sign = \"Gemini\";\n          }\n        }\n\n        if (month === 5) {\n          if (date >= 20 && date <= 30) {\n            this.sign = \"Cancer\";\n          }\n        }\n        if (month === 6) {\n          if (date >= 1 && date <= 21) {\n            this.sign = \"Cancer\";\n          }\n        }\n\n        if (month === 6) {\n          if (date >= 22 && date <= 31) {\n            this.sign = \"Leo\";\n          }\n        }\n        if (month === 7) {\n          if (date >= 1 && date <= 21) {\n            this.sign = \"Leo\";\n          }\n        }\n\n        if (month === 7) {\n          if (date >= 22 && date <= 31) {\n            this.sign = \"Virgo\";\n          }\n        }\n        if (month === 8) {\n          if (date >= 1 && date <= 21) {\n            this.sign = \"Virgo\";\n          }\n        }\n\n        if (month === 8) {\n          if (date >= 22 && date <= 30) {\n            this.sign = \"Libra\";\n          }\n        }\n        if (month === 9) {\n          if (date >= 1 && date <= 21) {\n            this.sign = \"Libra\";\n          }\n        }\n\n        if (month === 9) {\n          if (date >= 22 && date <= 31) {\n            this.sign = \"Scorpio\";\n          }\n        }\n        if (month === 10) {\n          if (date >= 1 && date <= 20) {\n            this.sign = \"Scorpio\";\n          }\n        }\n\n        if (month === 10) {\n          if (date >= 21 && date <= 30) {\n            this.sign = \"Sagittarius\";\n          }\n        }\n        if (month === 11) {\n          if (date >= 1 && date <= 20) {\n            this.sign = \"Sagittarius\";\n          }\n        }\n\n        if (month === 11) {\n          if (date >= 21 && date <= 31) {\n            this.sign = \"Capricorn\";\n          }\n        }\n        if (month === 0) {\n          if (date >= 1 && date <= 18) {\n            this.sign = \"Capricorn\";\n          }\n        }\n\n        this.horoscope = \"Your astrological zodiac sign is \" + this.sign + \".\";\n      }\n    },\n\n    selectUs() {\n      this.us.select = !this.us.select;\n      this.age = \"\";\n      this.zodiac = \"\";\n      this.horoscope = \"\";\n    },\n  },\n};\n</script>\n\n<template>\n  <section class=\"container\">\n    <div class=\"options\">\n      <button @click=\"selectUs\">Click Here</button>\n    </div>\n\n    <form v-if=\"us.select\">\n      <label>Enter birth date:</label>\n      <input type=\"date\" v-model=\"birthday\" :placeholder=\"us.birthday\" />\n      <button class=\"calculate\" @click.prevent=\"calculateAge\">\n        Calculate Age\n      </button>\n      <button class=\"calculate\" @click.prevent=\"getZodiacSign\">\n        Chinese Zodiac\n      </button>\n      <button class=\"calculate\" @click.prevent=\"getHoroscope\">Horoscope</button>\n    </form>\n\n    <h1>{{ age }}</h1>\n    <h1>{{ zodiac }}</h1>\n    <h1>{{ horoscope }}</h1>\n  </section>\n</template>\n\n\n<style scoped>\nhtml,\n* {\n  font-size: x-large;\n}\n\n.container {\n  display: flex;\n  width: 60vh;\n  margin: 0 auto;\n  flex-direction: column;\n  justify-content: space-between;\n  align-items: center;\n  padding: 10vh;\n  border-radius: 2vh;\n  background: rgb(168, 186, 224, 0.24);\n}\n\n.options {\n  margin: 0 auto;\n  width: 50vh;\n  display: flex;\n  justify-content: space-around;\n}\n\nbutton {\n  height: 8vh;\n  width: 18vh;\n  border-radius: 1vh;\n  font-size: 3vh;\n  font-weight: 600;\n  background-color: whitesmoke;\n  color: #000000;\n}\n\nbutton:focus {\n  color: whitesmoke;\n  background-color: #2c3e509f;\n}\n\nform {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  margin: 5vh;\n  color: black;\n}\n\n.calculate {\n  margin-top: 2vh;\n}\n\n@media (max-width: 500px) {\n  .container,\n  * {\n    max-width: 40vh;\n  }\n}\n</style>"]}]}