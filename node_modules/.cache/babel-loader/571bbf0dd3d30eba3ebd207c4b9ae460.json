 \"Horse\";\n        } else if (calc === 11) {\n          this.animal = \"Sheep\";\n        }\n        this.zodiac = \"Your Chinese zodiac sign is \" + this.animal + \".\";\n      }\n    },\n\n    getHoroscope() {\n      if (this.us.select) {\n        let birthDate = new Date(this.birthday);\n        var month = birthDate.getMonth();\n        var date = birthDate.getDate();\n\n        if (month === 0) {\n          if (date >= 19 && date <= 31) {\n            this.sign = \"Aquarius\";\n          }\n        }\n        if (month === 1) {\n          if (date >= 1 && date <= 17) {\n            this.sign = \"Aquarius\";\n          }\n        }\n\n        if (month === 1) {\n          if (date >= 18 && date <= 29) {\n            this.sign = \"Pisces\";\n          }\n        }\n        if (month === 2) {\n          if (date >= 1 && date <= 19) {\n            this.sign = \"Pisces\";\n          }\n        }\n\n        if (month === 2) {\n          if (date >= 20 && date <= 31) {\n            this.sign = \"Aries\";\n          }\n        }\n        if (month === 3) {\n          if (date >= 1 && date <= 18) {\n            this.sign = \"Aries\";\n          }\n        }\n\n        if (month === 3) {\n          if (date >= 19 && date <= 30) {\n            this.sign = \"Taurus\";\n          }\n        }\n        if (month === 4) {\n          if (date >= 1 && date <= 19) {\n            this.sign = \"Taurus\";\n          }\n        }\n\n        if (month === 4) {\n          if (date >= 20 && date <= 31) {\n            this.sign = \"Gemini\";\n          }\n        }\n        if (month === 5) {\n          if (date >= 1 && date <= 19) {\n            this.sign = \"Gemini\";\n          }\n        }\n\n        if (month === 5) {\n          if (date >= 20 && date <= 30) {\n            this.sign = \"Cancer\";\n          }\n        }\n        if (month === 6) {\n          if (date >= 1 && date <= 21) {\n            this.sign = \"Cancer\";\n          }\n        }\n\n        if (month === 6) {\n          if (date >= 22 && date <= 31) {\n            this.sign = \"Leo\";\n          }\n        }\n        if (month === 7) {\n          if (date >= 1 && date <= 21) {\n            this.sign = \"Leo\";\n          }\n        }\n\n        if (month === 7) {\n          if (date >= 22 && date <= 31) {\n            this.sign = \"Virgo\";\n          }\n        }\n        if (month === 8) {\n          if (date >= 1 && date <= 21) {\n            this.sign = \"Virgo\";\n          }\n        }\n\n        if (month === 8) {\n          if (date >= 22 && date <= 30) {\n            this.sign = \"Libra\";\n          }\n        }\n        if (month === 9) {\n          if (date >= 1 && date <= 21) {\n            this.sign = \"Libra\";\n          }\n        }\n\n        if (month === 9) {\n          if (date >= 22 && date <= 31) {\n            this.sign = \"Scorpio\";\n          }\n        }\n        if (month === 10) {\n          if (date >= 1 && date <= 20) {\n            this.sign = \"Scorpio\";\n          }\n        }\n\n        if (month === 10) {\n          if (date >= 21 && date <= 30) {\n            this.sign = \"Sagittarius\";\n          }\n        }\n        if (month === 11) {\n          if (date >= 1 && date <= 20) {\n            this.sign = \"Sagittarius\";\n          }\n        }\n\n        if (month === 11) {\n          if (date >= 21 && date <= 31) {\n            this.sign = \"Capricorn\";\n          }\n        }\n        if (month === 0) {\n          if (date >= 1 && date <= 18) {\n            this.sign = \"Capricorn\";\n          }\n        }\n\n        this.horoscope = \"Your astrological zodiac sign is \" + this.sign + \".\";\n      }\n    },\n\n    selectUs() {\n      this.us.select = !this.us.select;\n      this.age = \"\";\n      this.zodiac = \"\";\n      this.horoscope = \"\";\n    },\n  },\n};\n</script>\n\n<template>\n  <section class=\"container\">\n    <div class=\"options\">\n      <button @click=\"selectUs\">Click Here</button>\n    </div>\n\n    <for